@model IEnumerable<BPOR.Rms.Models.Volunteer.EnrollmentDetails>

<table class="govuk-table" style="width: 100%;">
    <thead class="govuk-table__head">
    <tr class="govuk-table__row">
        <th scope="col" class="govuk-table__header">Number recruited</th>
        <th scope="col" class="govuk-table__header">Date added</th>
    </tr>
    </thead>
    <tbody class="govuk-table__body">
    @foreach (var detail in Model)
    {
        <tr class="govuk-table__row">
            <td class="govuk-table__cell">
                @detail.RecruitmentTotal
            </td>
            <td class="govuk-table__cell">
                @detail.CreatedAt.ToString("dd/MM/yyyy")
            </td>
        </tr>
    }
    </tbody>
</table>
