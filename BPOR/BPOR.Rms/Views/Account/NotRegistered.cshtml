@{
    ViewData["Title"] = "Account Not Registered";
}
<div class="govuk-grid-row">
    <div class="govuk-grid-column-one-half">
        <h1 class="govuk-heading-l">Account not registered</h1>

        <div>
            <p class="govuk-body">
                Your details are not associated with an account in the Registry Management System.
            </p>
            <p class="govuk-body">
                Please contact <a class="govuk-link" href="mailto:crn.servicedesk@nihr.ac.uk">
                    crn.servicedesk@nihr.ac.uk
                </a> for further assistance.
            </p>
        </div>
    </div>
</div>

<div class="govuk-grid-row govuk-!-margin-top-4">
    <div class="govuk-grid-column-one-half">
        <a href="?sign-out" role="button" draggable="false" class="govuk-button govuk-button--start" data-module="govuk-button">
            Sign out of RMS
            <svg class="govuk-button__start-icon" xmlns="http://www.w3.org/2000/svg" width="17.5" height="19" viewBox="0 0 33 40" aria-hidden="true" focusable="false">
                <path fill="currentColor" d="M0 0h13l20 20-20 20H0l20-20z" />
            </svg>
        </a>
    </div>
</div>
